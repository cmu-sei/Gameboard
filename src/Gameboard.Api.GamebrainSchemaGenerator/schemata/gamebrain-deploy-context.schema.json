{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "IGamebrainDeployContext",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "game",
    "teams"
  ],
  "properties": {
    "game": {
      "$ref": "#/definitions/IGamebrainDeployContextSimpleEntity"
    },
    "teams": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/IGamebrainDeployContextTeam"
      }
    }
  },
  "definitions": {
    "IGamebrainDeployContextSimpleEntity": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "id",
        "name"
      ],
      "properties": {
        "id": {
          "type": "string",
          "minLength": 1
        },
        "name": {
          "type": "string",
          "minLength": 1
        }
      }
    },
    "IGamebrainDeployContextTeam": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "id",
        "name",
        "session",
        "gamespaces"
      ],
      "properties": {
        "id": {
          "type": "string",
          "minLength": 1
        },
        "name": {
          "type": "string",
          "minLength": 1
        },
        "session": {
          "$ref": "#/definitions/IGamebrainDeployContextSession"
        },
        "gamespaces": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/IGamebrainDeployContextGamespace"
          }
        }
      }
    },
    "IGamebrainDeployContextSession": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "start",
        "end"
      ],
      "properties": {
        "start": {
          "type": "string",
          "format": "date-time",
          "minLength": 1
        },
        "end": {
          "type": "string",
          "format": "date-time",
          "minLength": 1
        }
      }
    },
    "IGamebrainDeployContextGamespace": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "id",
        "vmUris"
      ],
      "properties": {
        "id": {
          "type": "string",
          "minLength": 1
        },
        "vmUris": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    }
  }
}